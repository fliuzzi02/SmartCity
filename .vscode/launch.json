    "version": "0.2.0",
    "configurations": [

        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Example",
            "request": "launch",
            "mainClass": "main.java.example.Example",
            "projectName": "SmartCity",
            "args": ["arg1", "arg2"]
        },
        {
            "type": "java",
            "name": "Congestion Scenario",
            "request": "launch",
            "mainClass": "main.java.utils.Main",
            "projectName": "SmartCity",
            "args": ["--congestion", 20, "R1s1", 0]
        },
        {
            "type": "java",
            "name": "Info Panel Testing",
            "request": "launch",
            "mainClass": "main.java.utils.Main",
            "projectName": "SmartCity",
            "args": ["--infoPanelTesting"]
        },
        {
            "type": "java",
            "name": "Accident Scenario",
            "request": "launch",
            "mainClass": "main.java.utils.Main",
            "projectName": "SmartCity",
            "args": ["--accident"]
        },
        {
            "type": "java",
            "name": "Create Managers",
            "request": "launch",
            "mainClass": "main.java.utils.Main",
            "projectName": "SmartCity",
            "args": ["--createManagers"]
        },
        {
            "type": "java",
            "name": "Create Info Panel",
            "request": "launch",
            "mainClass": "main.java.utils.Main",
            "projectName": "SmartCity",
            "args": ["--createInfoPanel", "R5s1", 290]
        },
        {
            "type": "java",
            "name": "Create Vehicle",
            "request": "launch",
            "mainClass": "main.java.utils.Main",
            "projectName": "SmartCity",
            "args": ["--createVehicle", "6942ABC", "PrivateUsage", 50, "R5s1,0,580;"]
        }
    ]
}